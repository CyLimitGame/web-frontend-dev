    "babel-plugin-transform-remove-imports": "^1.7.0",
    "chakra-dayzed-datepicker": "^0.2.1",
    "dayjs": "^1.10.7",
    "dayzed": "^3.2.3",
    "dotenv": "^10.0.0",
    "framer-motion": "^6",
    "gapi-script": "^1.2.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "12.3.1",
    "next-i18next": "^11.3.0",
    "next-pwa": "^5.2.21",
    "next-remove-imports": "^1.0.11",
    "nextjs-progressbar": "^0.0.14",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
